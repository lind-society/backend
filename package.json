{
  "name": "main",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start main",
    "start:message-queue": "nest start message-queue",
    "start:mail": "nest start mail",
    "start:scheduler": "nest start scheduler",
    "start:whatsapp": "nest start whatsapp",
    "start:dev": "nest start main --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/main/main",
    "lint": "eslint \"{@apps/main,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main/test/jest-e2e.json",
    "typeorm": "pnpm build && npx typeorm",
    "migration:run": "pnpm typeorm migration:run -d dist/apps/main/apps/main/src/database/data-source.js",
    "migration:generate": "pnpm typeorm migration:generate -d dist/apps/main/apps/main/src/database/data-source.js apps/main/src/database/migrations/migration",
    "migration:create": "pnpm typeorm migration:create @apps/main/database/migrations/migration",
    "migration:revert": "pnpm typeorm migration:revert -d dist/apps/main/apps/main/src/database/data-source.js",
    "migration:show": "pnpm typeorm migration:show -d dist/apps/main/apps/main/src/database/data-source.js",
    "migration:reset": "pnpm typeorm schema:drop -d dist/apps/main/apps/main/src/database/data-source.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.15.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.0.13",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.7",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.10.2",
    "nestjs-paginate": "^11.0.1",
    "nodemailer": "^7.0.2",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.20",
    "whatsapp-web.js": "^1.27.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": "22.16.0",
    "pnpm": "10.12.1"
  },
  "packageManager": "pnpm@10.12.1",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@app/mail-client(|/.*)$": "<rootDir>/libs/mail-client/src/$1",
      "^@app/message-queue-client(|/.*)$": "<rootDir>/libs/message-queue-client/src/$1",
      "^@app/rabbitmq(|/.*)$": "<rootDir>/libs/rabbitmq/src/$1",
      "^@app/scheduler-client(|/.*)$": "<rootDir>/libs/scheduler-client/src/$1",
      "^@app/whatsapp-client(|/.*)$": "<rootDir>/libs/whatsapp-client/src/$1"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "@swc/core",
      "bcrypt",
      "puppeteer"
    ]
  }
}
